{
  "name": "n8n-nodes-paperless-ngx",
  "version": "0.1.0",
  "description": "Custom n8n node for Paperless ngx with enhanced features for document management",
  "keywords": [
    "n8n-community-node-package",
    "paperless-ngx",
    "document-management",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://github.com/terschawebIT/n8n-nodes-paperless-ngx",
  "author": {
    "name": "Niko Terschawetz",
    "email": "niko.terschawetz@terschaweb.it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terschawebIT/n8n-nodes-paperless-ngx.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@10.17.0",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PaperlessNgxApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PaperlessNgx/PaperlessNgx.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.82.0"
  },
  "pnpm": {
    "overrides": {
      "axios": "^1.12.0",
      "form-data": "^4.0.4",
      "micromatch": "^4.0.8",
      "brace-expansion@1": "^1.1.12",
      "brace-expansion@2": "^2.0.2"
    }
  }
}
